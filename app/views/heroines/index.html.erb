<h2>All Heroines</h2>

<%= form_with url: heroines_path, method: 'get' do |f| %>
  <%= f.text_field :search %>
  <%= f.submit %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
<a class="button" href="heroines/new">Create a new Heroine</a>